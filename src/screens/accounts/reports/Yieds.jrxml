  UNION ALL
  Select IFNULL(sum(cast(`yields_accounts`.`amount` as UNSIGNED)),0)  as hr FROM `yields_accounts` WHERE  `yields_accounts`.`date`>=$P{date_from}  and `yields_accounts`.`date` <=$P{date_to} 
